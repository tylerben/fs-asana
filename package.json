{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "bin": {
    "fs-asana": "./dist/index.js"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "fs-asana": "node ./dist/index.js"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "fs-asana",
  "author": "Ben Tyler",
  "module": "dist/fs-asana.esm.js",
  "size-limit": [
    {
      "path": "dist/fs-asana.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/fs-asana.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.1",
    "@types/yargs": "^17.0.2",
    "husky": "^7.0.1",
    "size-limit": "^5.0.1",
    "ts-node": "^10.0.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "asana": "^0.18.6",
    "inquirer": "^8.1.1",
    "yargs": "^17.0.1"
  }
}
